{
  "expo": {
    "name": "Capstone",
    "slug": "capstone",
    "scheme": "com.proyectoespecialidad.arcoprime",
    "privacy": "hidden",
    "owner": "proyectoespecialidad",
    "version": "0.0.1",
    "description": "Grupo 8's mobile application",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "primaryColor": "#f0f0f0",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f0f0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.proyectoespecialidad.arcoprime",
      "supportsTablet": true
    },
    "android": {
      "package": "com.proyectoespecialidad.arcoprime",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#f0f0f0"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
